    <script defer type="text/javascript" src="{{ '/assets/node_modules/jquery/dist/jquery.min.js' | relative_url }}"></script>
    <script defer type="text/javascript" src="{{ '/assets/js/sidebar.js' | relative_url }}"></script>
    <script defer type="text/javascript" src="{{ '/assets/js/date.js' | relative_url }}"></script>
