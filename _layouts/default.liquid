<!DOCTYPE html>
<html>

  <head>
    {% seo %}
    <link type="text/css" rel="stylesheet" href="{{ '/assets/css/main.min.css' | relative_url }}" />
    <link rel="Shortcut Icon" href="{{ '/assets/img/favicon.ico' | relative_url }}" type="image/x-icon" />
    <link rel="search" href="{{ '/assets/xml/opensearch.xml' | relative_url }}"  title="Search {{ site.title }}" type="application/opensearchdescription+xml" />
  </head>

  <body>
    {% include components/modals/modal.liquid %}

    <section id="container" class="d-flex">

      <section id="generals" class="d-print-none">
        <div id="sidebar">
          {% include components/sidebars/sidebar.liquid %}
        </div>
      </section>

      <article id="content">
        {% for lang in site.languages %}
          <div id="{{ lang }}">
            {% include components/landing-pages/cv.liquid lang=lang %}
          </div>
        {% endfor %}
      </article>

    </section>

    {% include components/ribbons/octocat-corners.liquid %}
    {% include components/scripts/scripts.liquid %}
  </body>

</html>
